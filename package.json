  "scripts": {
    "start": "node exec.js local.env && react-scripts start",
    "build:clean": "rimraf ./build",
    "build": "node exec.js prod.env && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "CI=true npm run test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint:check": "eslint . --ext=ts,tsx",
    "lint:fix": "eslint . --ext=ts,tsx --fix;  exit 0",
    "build-package-css": "cp src/assets/css/material-dashboard-react.css dist/material-dashboard-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist",
    "deploy": "npm run predeploy && npm run deploy:s3:temp && npm run deploy:s3 && npm run deploy:s3:deltemp && npm run postdeploy:s3",
    "predeploy": "npm run build:clean && npm run lint:check && npm run test:ci && npm run build",
    "deploy:s3:temp": "aws s3 sync build/ s3://facetsuitemp --delete",
    "deploy:s3": "aws s3 sync s3://facetsuitemp s3://muiditorweb --delete",
    "deploy:s3:deltemp": "aws s3 rm s3://facetsuitemp --recursive",
    "postdeploy:s3": "aws cloudfront create-invalidation --distribution-id E2S0NGZ7OWUNSX --paths \"/*\"",
    "sitemap": "babel-node sitemap-generator.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikom/mobile-ui-baker.git"
  },
  "keywords": [],
  "license": "MIT",
  "optionalDependencies": {
    "@types/googlemaps": "3.30.16",
